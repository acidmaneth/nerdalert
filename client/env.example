# NerdAlert Frontend Environment Variables

# API Configuration
VITE_NERDALERT_API_URL=https://your-api-domain.com
# For production, use your deployed agent URL:
# VITE_NERDALERT_API_URL=https://your-deployed-agent.com

# WalletConnect Configuration
# Get your Project ID from: https://cloud.walletconnect.com
VITE_WALLET_PROJECT_ID=your_walletconnect_project_id_here

# Development Settings
NODE_ENV=development
VITE_APP_TITLE=NerdAlert Chat
VITE_APP_DESCRIPTION=Cyberpunk AI Chat Interface

# Optional: Custom API endpoints
VITE_API_BASE_URL=http://localhost:80
VITE_WS_URL=ws://localhost:80

# Show/hide WalletConnect button
VITE_WALLET_ENABLED=false

# Vite Environment Variables
# All environment variables must be prefixed with VITE_ to be available in the browser

# Development Settings
VITE_DEV_MODE=true
VITE_DEBUG_LEVEL=info

# Note: Only variables prefixed with VITE_ will be available in your React app
# Regular environment variables (without VITE_) are only available on the server

# WalletConnect Configuration
# Get your Project ID from: https://cloud.walletconnect.com
VITE_WALLET_PROJECT_ID=your_walletconnect_project_id_here

# Development Settings
NODE_ENV=development
VITE_APP_TITLE=NerdAlert Chat
VITE_APP_DESCRIPTION=Cyberpunk AI Chat Interface

# Optional: Custom API endpoints
VITE_API_BASE_URL=http://localhost:80
VITE_WS_URL=ws://localhost:80

# Show/hide WalletConnect button
VITE_WALLET_ENABLED=false 